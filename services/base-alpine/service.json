{
  "org": "${HZN_ORG_ID}",
  "label": "base",
  "description": "Provides base container with Alpine LINUX",
  "documentation": "https://github.com/dcmartin/open-horizon/blob/master/services/base-alpine/README.md",
  "url": "com.github.dcmartin.open-horizon.base-alpine",
  "version": "0.2.0",
  "arch": null,
  "public": false,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [
    { "name": "BASE_PERIOD", "label": "periodicity", "type": "int", "defaultValue": "30" },
    { "name": "SERVICE_LOG_LEVEL", "label": "specify logging level", "type": "string", "defaultValue": "info" }
  ],
  "deployment": {
    "services": {
      "base": {
        "environment": [
          "SERVICE_LABEL=base",
          "SERVICE_VERSION=0.2.0"
        ],
        "specific_ports": [],
        "image": null,
        "privileged": true
      }
    }
  },
  "tmpfs": {
    "size": 8192000
  }
}
