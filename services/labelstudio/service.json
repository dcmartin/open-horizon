{
  "org": "${HZN_ORG_ID}",
  "label": "labelstudio",
  "description": "",
  "documentation": "",
  "url": "addon-labelstudio",
  "version": "0.0.1",
  "arch": null,
  "public": false,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [
    {
      "name": "SERVICE_LOG_LEVEL",
      "label": "specify logging level",
      "type": "string",
      "defaultValue": "debug"
    }
  ],
  "deployment": {
    "services": {
      "labelstudio": {
        "environment": [
          "SERVICE_LABEL=labelstudio",
          "SERVICE_PORT=9002",
          "SERVICE_VERSION=0.0.1"
        ],
        "specific_ports": [
          {
            "HostPort": "9002:9002/tcp",
            "HostIP": "0.0.0.0"
          },
          {
            "HostPort": "7998:7998/tcp",
            "HostIP": "0.0.0.0"
          }
        ],
        "devices": [],
        "image": null,
        "privileged": true
      }
    }
  },
  "mount": [],
  "tmpfs": {
    "size": 81920000
  },
  "addon": {
    "name": "Label Studio",
    "version": "0.0.1",
    "slug": "labelstudio",
    "description": "Label Studio from labelstud.io",
    "url": "https://github.com/dcmartin/open-horizon/tree/master/services/labelstudio/README.md",
    "webui": "http://[HOST]:[PORT:7998]",
    "startup": "application",
    "boot": "auto",
    "arch": [
      "amd64",
      "aarch64"
    ],
    "panel_admin": true,
    "panel_icon": "mdi:label",
    "stage": "experimental",
    "devices": [],
    "hassio_api": false,
    "hassio_role": "default",
    "host_network": true,
    "homeassistant_api": false,
    "privileged": [],
    "timeout": 90,
    "tmpfs": true,
    "map": [],
    "ports": {
      "7998/tcp": 7998
    },
    "ports_description": {
      "7998/tcp": "Web interface"
    },
    "options": {
      "log_level": "info",
      "timezone": "America/Los_Angeles",
      "workspace": "/data/labelstudio",
      "project": "MyProject"
    },
    "schema": {
      "log_level": "str",
      "timezone": "str?",
      "workspace": "str?",
      "project": "str?",
      "username": "str?",
      "password": "str?",
      "init": "str?"
    },
    "environment": {
      "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
    },
    "image": "dcmartin/{arch}-labelstudio",
    "watchdog": "http://[HOST]:[PORT:7998]/api/health"
  }
}
